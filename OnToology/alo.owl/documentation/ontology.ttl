      dc:date "2014-09-15"^^xsd:dateTime ;
      dc:identifier "http://vocab.linkeddata.es/datosabiertos/def/turismo/alojamiento"^^xsd:dateTime ;
      dc:rights "Creative Commons Attribution 4.0 International (CC BY 4.0)" ;
